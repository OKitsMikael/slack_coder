# Erlang version
erlang_version=18.0

# Elixir version
elixir_version=1.2.0

# Always rebuild from scratch on every deploy?
always_rebuild=true

# Export heroku config vars
config_vars_to_export=(DATABASE_URL SLACK_API_TOKEN GITHUB_PAT GITHUB_USER)

# A command to run right after compiling the app
post_compile="pwd"
